<?xml version="1.0" encoding="UTF-8"?>
<ti:app xmlns:ti="http://ti.appcelerator.org" xmlns:appc="http://www.appcelerator.org">
    
	<id>com.titaniumapp.contactmanager</id>
	<name>contactmanager</name>
	<description>Titanium-based Contact Manager demo app</description>
	<copyright>Copyright (c) 2008 Appcelerator, Inc.</copyright>
	<homepage>http://titaniumapp.com</homepage>
	<updatesite>http://updatesite.titaniumapp.com</updatesite>
	<version>0.1</version>

	<window>
		<id>initial</id>
		<title>Welcome to Contact Manager</title>
		<url>app://index.html</url>
		<width>800</width>
		<min-width>0</min-width>
		<max-width>9000</max-width>
		<height>600</height>
		<min-height>0</min-height>
		<max-height>9000</max-height>
		<maximizable>false</maximizable>
		<minimizable>false</minimizable>
		<closeable>false</closeable>
		<resizable>true</resizable>
		<chrome scrollbars="false">true</chrome>
		<transparency>.99</transparency>
	</window>

	<icon>
		<image16>cm16.png</image16>
		<image32>cm32.png</image32>
		<image48>cm48.png</image48>
	</icon>

</ti:app>
